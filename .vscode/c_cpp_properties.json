{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/kernel",
                "${workspaceFolder}/kernel/Core",
                "${workspaceFolder}/out/gen"
            ],
            "defines": [],
            "macFrameworkPath": [],
            "intelliSenseMode": "gcc-x86",
            //"compilerPath": ["/usr/local/osdev/bin/i386-elf-gcc", "D:/Programs/gcc/bin/i386-elf-g++.exe"],
            "compilerPath": "D:/Programs/gcc/bin/i386-elf-g++.exe",
            "cStandard": "c11",
            "cppStandard": "gnu++14",
            "compilerArgs": [
				"-include kernel/global.h",
				"-std=gnu++1z",
				"-mgeneral-regs-only",
				"-fno-isolate-erroneous-paths-attribute",
				"-fno-asynchronous-unwind-tables"
            ]
        }
    ],
    "version": 4
}